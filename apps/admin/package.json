{
  "name": "admin",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "npm run build:css && remix build",
    "build:css": "tailwindcss -m -i ./styles/app.css -o app/styles/app.css",
    "dev": "concurrently \"npm run dev:css\" \"remix dev\"",
    "dev:css": "tailwindcss -w -i ./styles/app.css -o app/styles/app.css",
    "lint": "eslint app *.ts*"
  },
  "dependencies": {
    "@headlessui/react": "^1.6.5",
    "@heroicons/react": "^1.0.6",
    "@prisma/client": "^3.15.2",
    "@remix-run/node": "^1.6.1",
    "@remix-run/react": "^1.6.1",
    "@remix-run/vercel": "^1.6.1",
    "@turbyo/api": "workspace:^",
    "@turbyo/ui": "workspace:^0.0.0",
    "@vercel/node": "^2.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tiny-invariant": "^1.2.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.6.1",
    "@remix-run/eslint-config": "^1.6.1",
    "@remix-run/serve": "^1.6.1",
    "@tailwindcss/forms": "^0.5.2",
    "@turbyo/eslint-config": "workspace:^0.0.0",
    "@turbyo/ts-config": "workspace:*",
    "@types/node": "^18.0.0",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "concurrently": "^7.2.2",
    "eslint": "^8.18.0",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.11",
    "tailwindcss": "^3.1.4",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.4"
  },
  "engines": {
    "node": ">=14"
  },
  "installConfig": {
    "hoistingLimits": "workspaces"
  }
}
